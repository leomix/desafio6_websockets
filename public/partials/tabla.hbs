{{#if productos.length }}
    <table class="table table-dark">
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Foto URL</th>
            </tr>
            {{#each productos}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.nombre}}</td>
                <td>${{this.precio}}</td>
                <td><img src={{this.url}}></td>
            </tr>
            {{/each}}
    </table>
{{/if}}